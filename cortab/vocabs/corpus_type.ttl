        <https://core.clscor.io/entity/type/corpus_type/general>,
        <https://core.clscor.io/entity/type/corpus_type/learner>,
        <https://core.clscor.io/entity/type/corpus_type/manually_annotated>,
        <https://core.clscor.io/entity/type/corpus_type/monitor>,
        <https://core.clscor.io/entity/type/corpus_type/monolingual>,
        <https://core.clscor.io/entity/type/corpus_type/multilingual>,
        <https://core.clscor.io/entity/type/corpus_type/opportunistic>,
        <https://core.clscor.io/entity/type/corpus_type/parallel>,
        <https://core.clscor.io/entity/type/corpus_type/programmable>,
        <https://core.clscor.io/entity/type/corpus_type/reference>,
        <https://core.clscor.io/entity/type/corpus_type/representative>,
        <https://core.clscor.io/entity/type/corpus_type/specialized>,
        <https://core.clscor.io/entity/type/corpus_type/spoken>,
        <https://core.clscor.io/entity/type/corpus_type/synchronic>,
        <https://core.clscor.io/entity/type/corpus_type/web_corpus>,
        <https://core.clscor.io/entity/type/corpus_type/written> .


