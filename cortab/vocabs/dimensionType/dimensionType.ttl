@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix crmcls: <https://clscor.io/ontologies/CRMcls/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .	
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

#dimensionType vocabulary
#define all skos:Concepts first

<https://core.clscor.io/entity/type/dimension_type/texts> a skos:Concept ; 
	skos:prefLabel "texts"@en ;
	skos:definition "Count of texts in a corpus." .

<https://core.clscor.io/entity/type/dimension_type/words> a skos:Concept ; 
	skos:prefLabel "words"@en ;
	skos:definition "Count of words in a corpus." .	

#define skos:conceptScheme

<https://core.clscor.io/entity/type/dimension_type/> a skos:ConceptScheme .

#add skos:Concepts to skos:ConceptScheme

<https://core.clscor.io/entity/type/dimension_type/> skos:hasTopConcept
	<https://core.clscor.io/entity/type/dimension_type/texts> ,
	<https://core.clscor.io/entity/type/dimension_type/words> .
