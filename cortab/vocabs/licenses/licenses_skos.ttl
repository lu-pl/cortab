@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix crmcls: <https://clscor.io/ontologies/CRMcls/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

#licenses vocabulary
#define all skos:Concepts first

<https://core.clscor.io/entity/type/license/CC-0-1.0> a skos:Concept ;
	skos:prefLabel "Creative Commons Zero v1.0 Universal"@en ;
	skos:exactMatch <https://creativecommons.org/publicdomain/zero/1.0/legalcode> ,
	<http://spdx.org/licenses/CC0-1.0> , 
	<https://vocabs.acdh.oeaw.ac.at/archelicenses/cc0-1-0> .

<https://core.clscor.io/entity/type/license/CC-BY-4.0> a skos:Concept ;
	skos:prefLabel "Creative Commons Attribution 4.0 International"@en ;
	skos:exactMatch <https://creativecommons.org/licenses/by/4.0/legalcode> ,
	<http://spdx.org/licenses/CC-BY-4.0> , 
	<https://vocabs.acdh.oeaw.ac.at/archelicenses/cc-by-4-0> .
	
<https://core.clscor.io/entity/type/license/CC-BY-ND-4.0> a skos:Concept ;
	skos:prefLabel "Creative Commons Attribution No Derivatives 4.0 International"@en ;
	skos:exactMatch <https://creativecommons.org/licenses/by-nd/4.0/legalcode> ,
	<http://spdx.org/licenses/CC-BY-ND-4.0> , 
	<https://vocabs.acdh.oeaw.ac.at/archelicenses/cc-by-nd-4-0> .

	
#define skos:conceptScheme

<https://core.clscor.io/entity/type/license> a skos:ConceptScheme .

#add skos:Concepts to skos:ConceptScheme

<https://core.clscor.io/entity/type/license> skos:hasTopConcept 
    <https://core.clscor.io/entity/type/license/CC-0-1.0> ,
	<https://core.clscor.io/entity/type/license/CC-BY-4.0> ,
	<https://core.clscor.io/entity/type/license/CC-BY-ND-4.0> .
