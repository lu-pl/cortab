@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<https://core.clscor.io/entity/type/appellation_type/acronym> a skos:Concept ;
    rdfs:label "acronym" ;
    skos:definition "Abbreviated denomination of the corpus." ;
    skos:inScheme <https://core.clscor.io/entity/type/appellation_type/> ;
    skos:prefLabel "acronym" .

<https://core.clscor.io/entity/type/appellation_type/alternate_title> a skos:Concept ;
    rdfs:label "alternate title" ;
    skos:definition "Alternate title for the corpus." ;
    skos:inScheme <https://core.clscor.io/entity/type/appellation_type/> ;
    skos:prefLabel "alternate title" .

<https://core.clscor.io/entity/type/appellation_type/artificial_acronym> a skos:Concept ;
    rdfs:label "artificial acronym" ;
    skos:definition "Artificial abbreviated denomination of the corpus (introduced for corpora where no acronym is provided)." ;
    skos:inScheme <https://core.clscor.io/entity/type/appellation_type/> ;
    skos:prefLabel "artificial acronym" .

<https://core.clscor.io/entity/type/appellation_type/full_title> a skos:Concept ;
    rdfs:label "full title" ;
    skos:definition "Full title of the corpus." ;
    skos:inScheme <https://core.clscor.io/entity/type/appellation_type/> ;
    skos:prefLabel "full title" .

<https://core.clscor.io/entity/type/appellation_type/translated_full_title> a skos:Concept ;
    rdfs:label "translated full title" ;
    skos:definition "Translated full title of the corpus." ;
    skos:inScheme <https://core.clscor.io/entity/type/appellation_type/> ;
    skos:prefLabel "translated full title" .

<https://core.clscor.io/entity/type/appellation_type/> a skos:ConceptScheme ;
    skos:hasTopConcept <https://core.clscor.io/entity/type/appellation_type/acronym>,
        <https://core.clscor.io/entity/type/appellation_type/alternate_title>,
        <https://core.clscor.io/entity/type/appellation_type/artificial_acronym>,
        <https://core.clscor.io/entity/type/appellation_type/full_title>,
        <https://core.clscor.io/entity/type/appellation_type/translated_full_title> .


